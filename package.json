{
  "name": "@lunelson/sass-u",
  "version": "0.10.1",
  "description": "Collected Sass utility functions",
  "keywords": [
    "libsass",
    "sass",
    "utilities",
    "utility"
  ],
  "homepage": "https://github.com/lunelson/sass-u#readme",
  "bugs": {
    "url": "https://github.com/lunelson/sass-u/issues"
  },
  "license": "MIT",
  "author": "Lu Nelson",
  "main": "_index.scss",
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lunelson/sass-u.git"
  },
  "scripts": {
    "test": "ava",
    "start": "ava --watch"
  },
  "devDependencies": {
    "@lunelson/sass-throw": "^2.0.0-rc.4",
    "ava": "^1.0.1",
    "del": "^3.0.0",
    "node-sass": "^4.11.0",
    "sass": "^1.15.3",
    "write": "^1.0.3"
  },
  "peerDependencies": {
    "@lunelson/sass-throw": "^2.0.0-rc.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "ava": {
    "files": [
      "tests/*{.js,.scss}"
    ],
    "sources": [
      "_index.scss",
      "scss/**/*.scss",
      "!**/_*/**"
    ]
  }
}
